{
  "name": "rtyi-doc",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.12.1",
  "scripts": {
    "transcripts-extractor": "tsx --env-file=.env scripts/transcripts-extractor.ts",
    "generate-questions": "tsx --env-file=.env scripts/generate-questions.ts",
    "combine-stream-extracts": "tsx --env-file=.env scripts/combine-stream-extracts.ts",
    "combine-questions": "tsx --env-file=.env scripts/combine-questions.ts",
    "docs:dev": "pnpm -C docs dev",
    "docs:build": "pnpm -C docs build",
    "docs:preview": "pnpm -C docs preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"**/*.{css,html,json,md,mjs,ts,vue,yml}\" --write",
    "format:check": "prettier \"**/*.{css,html,json,md,mjs,ts,vue,yml}\" --check",
    "prepare": "wrangler types --path=\"./worker/worker-configuration.d.ts\""
  },
  "devDependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/google": "^1.2.19",
    "@ai-sdk/openai": "^1.3.22",
    "@antfu/eslint-config": "^4.14.1",
    "@clack/prompts": "^0.11.0",
    "@sindresorhus/slugify": "^2.2.1",
    "@types/node": "^22.15.32",
    "ai": "^4.3.16",
    "ansis": "^4.1.0",
    "eslint": "^9.29.0",
    "p-queue": "^8.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-vitepress": "^0.0.10",
    "tinyglobby": "^0.2.14",
    "tsx": "^4.20.3",
    "unstorage": "^1.16.0",
    "utilful": "^2.1.1",
    "wrangler": "^4.20.0",
    "zod": "^3.25.67",
    "zx": "^8.5.5"
  }
}
