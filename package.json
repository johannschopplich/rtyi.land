{
  "name": "rtyi-doc",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.30.1",
  "scripts": {
    "stream-analysis": "tsx --env-file=.env scripts/stream-analysis.ts",
    "stream-synthesis": "tsx --env-file=.env scripts/stream-synthesis.ts",
    "docs:dev": "pnpm -C docs dev",
    "docs:build": "pnpm -C docs build",
    "docs:preview": "pnpm -C docs preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"**/*.{css,html,json,md,mjs,ts,vue,yml}\" --write",
    "format:check": "prettier \"**/*.{css,html,json,md,mjs,ts,vue,yml}\" --check",
    "test:types": "tsc --noEmit"
  },
  "devDependencies": {
    "@ai-sdk/openai": "^3.0.30",
    "@antfu/eslint-config": "^7.4.3",
    "@clack/prompts": "^1.0.1",
    "@types/node": "^24.10.13",
    "ai": "^6.0.97",
    "ansis": "^4.2.0",
    "culori": "^4.0.2",
    "eslint": "^10.0.1",
    "p-map": "^7.0.4",
    "p-queue": "^9.1.0",
    "prettier": "^3.8.1",
    "prettier-plugin-vitepress": "^0.0.10",
    "tinyglobby": "^0.2.15",
    "tokenx": "^1.3.0",
    "tsx": "^4.21.0",
    "unstorage": "^1.17.4",
    "utilful": "^2.5.1",
    "wrangler": "^4.67.0",
    "zod": "^4.3.6",
    "zx": "^8.8.5"
  }
}
