{
  "name": "rtyi-doc",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.17.1",
  "scripts": {
    "stream-analysis": "tsx --env-file=.env scripts/stream-analysis.ts",
    "docs:dev": "pnpm -C docs dev",
    "docs:build": "pnpm -C docs build",
    "docs:preview": "pnpm -C docs preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"**/*.{css,html,json,md,mjs,ts,vue,yml}\" --write",
    "format:check": "prettier \"**/*.{css,html,json,md,mjs,ts,vue,yml}\" --check",
    "test:types": "tsc --noEmit",
    "prepare": "wrangler types --path=\"./worker/worker-configuration.d.ts\""
  },
  "devDependencies": {
    "@ai-sdk/anthropic": "^2.0.22",
    "@ai-sdk/google": "^2.0.17",
    "@ai-sdk/openai": "^2.0.41",
    "@antfu/eslint-config": "^5.4.1",
    "@clack/prompts": "^0.11.0",
    "@sindresorhus/slugify": "^3.0.0",
    "@types/node": "^24.6.0",
    "ai": "^5.0.59",
    "ansis": "^4.2.0",
    "eslint": "^9.36.0",
    "p-queue": "^8.1.1",
    "prettier": "^3.6.2",
    "prettier-plugin-vitepress": "^0.0.10",
    "tinyglobby": "^0.2.15",
    "tsx": "^4.20.6",
    "unstorage": "^1.17.1",
    "utilful": "^2.1.1",
    "wrangler": "^4.40.2",
    "zod": "^4.1.11",
    "zx": "^8.8.4"
  }
}
